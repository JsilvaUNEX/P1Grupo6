module RoboCompGrid2D
{
      struct TPoint
      {
            float x;
            float y;
            float radius;    // radius of the object's bounding box
      };

      sequence<TPoint> TPath;

      struct Result
      {
            TPath path;
            long timestamp;
            string errorMsg;
            bool valid;
      };

      interface Grid2D
      {
            Result getPaths(TPoint source, TPoint target);
      };
};
